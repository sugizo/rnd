  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.42">
    <script>
pipeline {
    agent any

    stages {
        stage(&apos;Remove App&apos;) {
            steps {
                sh &apos;&apos;&apos;
# Remove App
rm -rf /Applications/XAMPP/xamppfiles/htdocs/$app
&apos;&apos;&apos;
            }
        }
        stage(&apos;Install App&apos;) {
            steps {
                sh &apos;&apos;&apos;
# Install App
rsync -zavur ~/Cloud/MEGA/Git/codeigniter/$app /Applications/XAMPP/xamppfiles/htdocs/
&apos;&apos;&apos;
            }
        }
    }
}
</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <disabled>false</disabled>
